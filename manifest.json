{
   "manifest_version": 2,
   "name": "__MSG_app_name__",
   "short_name": "__MSG_app_short_name__",
   "description": "__MSG_app_description__",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "default_locale": "en",
   "version": "1.6.11",
   "author": "raingart",
   "permissions": [
      "storage"
   ],
   "background": {
      "page": "/html/background.html",
      "persistent": true
   },
   "content_scripts": [{
      "matches": ["https://www.youtube.com/*"],
      "run_at": "document_start",
      "all_frames": true,
      "js": [
         "/js/content/plugins-list.js",
         "/js/content/plugins.js",
         "/js/libs/storage.js",
         "/js/ytc_api_key_bridge.js",
         "/js/content/app.js"
      ]
   }],
   "web_accessible_resources": ["/plugins/*"],
   "icons": {
      "16": "/icons/16.png",
      "48": "/icons/48.png",
      "128": "/icons/128.png"
   },
   "options_page": "/html/options.html",
   "browser_action": {
      "default_icon": {
         "16": "/icons/16.png",
         "48": "/icons/48.png",
         "128": "/icons/128.png"
      },
      "default_popup": "/html/options.html"
   }
}
